@#?SuikaWiki/0.9
,urn:schemas-microsoft-com:asm.v1
,urn:schemas-microsoft-com:datatypes
,urn:schemas-microsoft-com:datatypes/[VAR[*]],[[DT4DTD]]
,urn:schemas-microsoft-com:exch-data
,urn:schemas-microsoft-com:fulltextqueryinfo
,urn:schemas-microsoft-com:office:excel,[[Excel]]
,urn:schemas-microsoft-com:office:forms,
,urn:schemas-microsoft-com:office:office,[[Office]]
,urn:schemas-microsoft-com:office:smarttags,[[SmartTag]]
