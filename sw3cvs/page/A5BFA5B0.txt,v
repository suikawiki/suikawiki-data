y = ( screen.height - 100 ) / 2;
r = y;
d = 0.0;
function circulate() {
moveTo( x + Math.cos( d ) * r, y - Math.sin( d ) * r );
d += Math.PI / 5.0;
if ( d >= Math.PI * 2 ) { d = 0.0; }
}
setInterval( 'circulate()', 100 );
