## いかれた UA から得た Referer 記録
Site:
  @@URIReferenceMatch:
    %uri (uri => {Field%20blocked%20by%20Outpost%20(http://www.agnitum.com)});
  @@URIReference2Label:
    Field blocked by Outpost (http://www.agnitum.com/)
Site:
  @@URIReferenceMatch:
    %uri (uri => {[unknown%20origin]});
  @@URIReference2Label:
    [unknown origin]

## Referer spam
Site:
  @@URIReferenceMatch:
    %or (
      x => {%host (name => {hq-inform.com});}p,
      {%host (name => {www.easythumbs.com});}p,
      x => {%host (name => {www.7voyeur-upskirt.com});}p,
      {%host (name => {mot.freetaboohost.com});}p,
      {%host (name => {www2.pics4all.net});}p,
      {%host (name => {www2.massearch.net});}p,
      {%host (name => {www2.sexunique.net});}p,
      {%host (name => {www2.teensearch.net});}p,
      {%host (name => {www2.2pursuit.com});}p,
      {%host (name => {www2.teenassearch.net});}p,
      x => {%host (name => {mature--young.com});}p,
      {%host (name => {www2.xfreehosting.com});}p,
      x => {%host (name => {www.sexxx-links.net});}p,
      {%host (name => {fhg.dacash.com});}p,
      x => {%host (name => {www.porn-o-rama.com});}p,
