      {%host (name => {ultram.250free.com});}p,
      {%host (name => {www.sexrabbit.de});}p,
      x => {%host (name => {gay.old-young.com});}p,
      x => {%host (name => {www.porn-o-rama.com});}p,
    );
  @@URIReference2Label:
    Referer spam: <%uri-reference;>

