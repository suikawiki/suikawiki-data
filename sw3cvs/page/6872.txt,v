,[CODE(HTMLa)[[[ondragleave]]]]	,[CODE(SGML)[%[[Script]]]]	,(なし)	,	,[WinIE]
,[CODE(HTMLa)[[[ondragover]]]]	,[CODE(SGML)[%[[Script]]]]	,(なし)	,	,[WinIE]
