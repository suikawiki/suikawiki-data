/* 段落 */
P		{ margin: 3px 0px 3px 0px; }

/* PRE と BLOCKQUOTE */
blockquote	{ border:1px solid #d0e0ff; background-color:#d0e0ff; margin:0.5em 5em; padding:0.25em; }
pre	{ color:#061542; background-color:#eeeeff; margin:0.5em 5em; padding:0.5em; border:thin dotted #9999ff; line-height: 100%/100%; }

/* リンク */
A:link    { color: #0000CC; }
A:visited { color: #000080; }
A:hover   { color: #0000FF; }
H2 A:link    { color: #FFFFFF; }
H2 A:visited { color: #FFFFFF; }

/* テーブル */
TABLE     { margin:5px; margin:0.5em 5em; border:1px solid #9999ff; background:#ccccff; }
TD        { margin:1px; padding:2px 10px; background:#ffffff; }

