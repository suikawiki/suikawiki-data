,checked 属性 >>1  ,"checked"  ,"checked"      ,HTML4
,[[class属性]]     ,           ,               ,HTML4
,[[dir属性]]       ,           ,               ,HTML4
,[[disabled属性]]  ,"disabled" ,"disabled"     ,HTML4
,[[id属性]]        ,           ,               ,HTML4
,[[ismap属性]]     ,           ,               ,HTML4
,[[lang属性]]      ,           ,               ,HTML4
