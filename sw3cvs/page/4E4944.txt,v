,	,[CODE(URI)@@en[[[urn:document]]]]	,	,"IANA 未登録, [[JIS TR]] 解説例"
,	,[CODE(URI)[[[urn:example]]]]	,例示	,	,非標準
,	,[[urn:example.microsoft.com]]	,例示	,M$	,[M$DN]
,	,[CODE(URI)@@en[[[urn:fdc]]:]]	,	,"[[IETF]] [[情報提供]], [[IANA]] 登録済み"	,[[RFC 4198]]
,	,[[urn:finance]]	,例示	,M$
