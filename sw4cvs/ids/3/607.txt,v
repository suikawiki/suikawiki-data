,[CODE(CSS)@@en[[[-moz-cjk-heavenly-stem]]]]	,[[十干]]
,[CODE(CSS)@@en[[[-moz-devanagari]]]]
,[CODE(CSS)@@en[[[-khtml-diamond]]]]
,[CODE(CSS)@@en[[[-moz-ethiopic-halehame]]]]
