]PRE]

[275] [[url]]
[PRE(code)[
[SPAN[-]] Drop any '''['''CODE(URI)@@en'''[''''''[''''''['''#form:[SPAN[']][SPAN[']]:[SPAN[']][SPAN[']]''']'''''']'''#''']'''''']'''''']'''''']''' and following characters.
]PRE]

[450] [[URI//punctuation]]
[PRE(code)[
中で使えます。 '''['''CODE(URI)'''[''''''[''''''['''#form:[SPAN[']][SPAN[']]:[SPAN[']][SPAN[']]''']'''''']'''#''']'''''']'''''']'''''']'''
]PRE]

[451] [[URI//reserved]]
[PRE(code)[
'''['''CODE(URI)'''[''''''[''''''['''#form:[SPAN[']][SPAN[']]:[SPAN[']][SPAN[']]''']'''''']'''#''']'''''']'''''']'''''']''' と
]PRE]

