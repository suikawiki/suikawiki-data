,[CODE(CSS)[[[display]]: [[-wap-marquee]]]]
,[CODE(CSS)[[[-wap-marquee-style]]: {[[scroll]]|[[slide]]|[[alternate]]}]]
,[CODE(CSS)[[[-wap-marquee-loop]]: {[[<integer>]]|[[infinite]]}]]
,[CODE(CSS)[[[-wap-marquee-dir]]: {[[ltr]]|[[rtl]]}]]
,[CODE(CSS)[[[-wap-marquee-speed]]: {[[slow]]|[[normal]]|[[fast]]}]]
,[CODE(CSS)[[[-wap-accesskey]]: {[[<KeyCombination>]]+|[[none]]|[[inherit]]}]]
,[CODE(CSS)[[[-wap-input-format]]: [[<format>]]]]
,[CODE(CSS)[[[-wap-input-required]]: {[[true]]|[[false]]}]]
